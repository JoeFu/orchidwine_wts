<div id="page-head">
  <div id="page-title">
    <h1 class="page-header text-overflow">管理员/销售 管理</h1>
  </div>
  <ol class="breadcrumb">
    <li><i class="ion-home"></i></li>
    <li>用户管理</li>
    <li class="active">管理员/销售 管理</li>
  </ol>
</div>
<div id="page-content">
  <div class="panel">
    <div class="panel-heading">
      <h3 class="panel-title">管理员/销售 列表</h3>
    </div>
    <div class="panel-body">
      <div class="pad-btm form-inline">
        <%= form_tag("/admin/admins", method: :get) do -%>
        <div class="row">
          <div class="col-sm-3 table-toolbar-left">
            <%== new_btn_tag "/admin/admins/new", '添加管理员' %>
          </div>
          <div class="col-sm-9 table-toolbar-right">
            <div class="form-group">
              <%= text_field_tag 'q', params[:q], placeholder: '搜索姓名或电话', class: "form-control"%>
            </div>
            <%== submit_tag %>
            <%== clear_tag "/admin/admins" %>
          </div>
        </div>
        <% end -%>
      </div>
      <%= render 'admin/admins/list' %>
      <div class="row">
        <%== table_footer_tag @admins %>
      </div>
    </div>
  </div>
</div>