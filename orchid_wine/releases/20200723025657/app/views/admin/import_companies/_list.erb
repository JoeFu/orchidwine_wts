<div class="table-responsive">
  <table class="table table-striped table-vcenter">
    <thead>
      <tr>
        <th>公司名称</th>
        <th>联系人</th>
        <th>电话号码</th>
        <th>电子邮箱</th>
        <th>社会统一信用代码</th>
        <th>地址</th>
        <th>销售</th>
        <th class="text-center" width="1%">操作</th>
        <th class="text-center" width="1%"></th>
      </tr>
    </thead>
    <tbody>
      <% @import_companies.map do |import_company|%>
      <tr>
        <td><%== import_company.name %></td>
        <td><%== import_company.contacts %></td>
        <td><%== import_company.telephone %></td>
        <td><%== import_company.email %></td>
        <td><%== import_company.uscc %></td>
        <td><%== import_company.address %></td>
        <td><%== import_company.admin.show_name rescue nil %></td>

        <td class="text-center">
          <%== edit_btn_tag(import_company) if can?(:edit, :ImportCompany) %>
        </td>
        <td class="text-center">
          <% if @admin_session.role < 2 %>
          <%== destroy_btn_tag(import_company) if can?(:destroy, :ImportCompany) %>
          <% end %>
        </td>
      </tr>
      <% end %>
    </tbody>
  </table>
</div>